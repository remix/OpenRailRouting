// to use this custom model you need to set the following option in the config.yml
// profiles:
//    - name: rail
//      turn_costs:
//        vehicle_types: []
//      custom_model_files: [anytrack.json]

{
  "distance_influence": 0,
  "priority": [
    { "if": "rail_access == CONSTRUCTION || rail_access == TRAM || rail_access == RAIL || rail_access == LIGHT_RAIL || rail_access == SUBWAY || rail_access == PROPOSED || rail_access == FUNICULAR || rail_access == MONORAIL", "multiply_by": "0" }
  ],
  "speed": [
    { "if": "true", "limit_to": "rail_average_speed" }
  ]
}
